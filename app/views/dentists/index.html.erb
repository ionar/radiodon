  <div class="row">
    <div class="col-lg-8 col-md-8 col-sm-8 col-xs-8">
      <h2><%= t("dentists") %></h2>
    </div>
    <div class="col-lg-4 col-md-4 col-sm-4 col-xs-4">
      <div class="text-right">
        <h2>
          <%= link_to new_dentist_path, :class => 'btn btn-primary' do %>
            <span class="glyphicon glyphicon-plus"></span> Adicionar
          <% end %>
        </h2>
      </div>
    </div>
  </div> <!-- /.row -->
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= t("name") %></th>
      <th><%= t("register") %></th>
      <th><%= t("email") %></th>
      <th><%= t("phone") %></th>
    </tr>
  </thead>

  <tbody>
    <% @dentists.each do |dentist| %>
      <tr>
        <td><%= link_to dentist.name, dentist %></td>
        <td><%= h(dentist.register) %></td>
        <td><%= dentist.email %></td>
        <td><%= dentist.phone %></td>
      </tr>
    <% end %>
  </tbody>
</table>
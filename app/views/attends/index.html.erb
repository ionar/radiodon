<h1><%= t("attends") %></h1>
<div class="text-right">
  <%= link_to new_attend_path, :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-plus"></span> Adicionar
  <% end %>
</div>

<div class="row">
  <div class="col-lg-4">
      <div id="events_calendar">
        <%= render partial: "calendar", locals: {attends: @attends_todos} %>
      </div><!-- /events_calendar -->
  </div><!-- /col -->
  <div class="col-lg-4">
    <div id="lista-horarios">
    <%= render partial: "listagem", locals: {attends: @attends} %>
    </div><!-- /lista-horarios -->
  </div><!-- /col -->
  <div class="col-lg-4">
    <div id="cliente-atendimento">
     <%= render partial: "cliente_atendimento", locals: {attends: @attends} %>
    </div><!-- /cadastra-ainda-nao-ta-usando -->
  </div><!-- /col -->  
</div><!-- /row -->

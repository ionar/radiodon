  <div class="row">
    <div class="col-lg-8 col-md-8 col-sm-8 col-xs-8">
      <h2><%= @clinic.fantasy_name %></h2>
    </div>
    <div class="col-lg-4 col-md-4 col-sm-4 col-xs-4">
      <div class="text-right">
        <h2>
          <%= link_to clinics_path, :class => 'btn btn-primary' do %>
            <span class="glyphicon glyphicon-chevron-left"></span> Voltar
          <% end %> 
          <%= link_to edit_clinic_path(@clinic), :class => 'btn btn-success' do %>
            <span class="glyphicon glyphicon-list-alt"></span> Editar
          <% end %> 
          <%= link_to clinic_path(@clinic), method: :delete, :class => 'btn btn-danger', data: { confirm: t('Are you sure?')  } do %>
            <span class="glyphicon glyphicon-remove"></span> Apagar
          <% end %> 
        </h2>
      </div>
    </div>
  </div> <!-- /.row -->
  <div class="panel panel-default">
      <div class="panel-body">
        <p>
          <strong>Fome Fantasia:</strong>
          <%= @clinic.fantasy_name %>
        </p>

        <p>
          <strong>Razão Social:</strong>
          <%= @clinic.corporate_name %>
        </p>
        <p>
          <strong>Fone:</strong>
          <%= @clinic.phone %>
        </p>
        <p>
          <strong>Cnpj:</strong>
          <%= @clinic.cnpj %>
        </p>

        <p>
          <strong>Responsável Técnico:</strong>
          <%= @clinic.technical_manager %>
        </p>

        <p>
          <strong>Endereço:</strong>
          <%= @clinic.address %>
        </p>

        <p>
          <strong>Cep:</strong>
          <%= @clinic.cep %>
        </p>

        <p>
          <strong>Cidade:</strong>
          <%= @clinic.city %>
        </p>

        <p>
          <strong>Estado:</strong>
          <%= @clinic.province.name %>
        </p>

        <p>
          <strong>Criado em:</strong>
          <%=l @clinic.created_at, format: :long %>
        </p>
        <p>
          <strong>Atualizado em:</strong>
          <%=l @clinic.updated_at, format: :long %>
        </p>
      </div><!-- /panel-body -->
    </div><!-- /panel -->